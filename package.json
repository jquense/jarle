{
  "name": "jarle",
  "version": "1.2.0",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/jquense/jarle.git"
  },
  "author": "Jason Quense <monastic.panic@gmail.com>",
  "license": "MIT",
  "scripts": {
    "add-import": "sed -i '' 's/__IMPORT__/(s) => import(\\/\\* webpackIgnore: true \\*\\/ s)/' ./{lib,esm}/Provider.js",
    "build": "4c build src && yarn add-import && node ./build-themes.js",
    "prepublishOnly": "yarn run build",
    "tdd": "jest --watch",
    "test": "jest"
  },
  "files": [
    "esm",
    "lib",
    "themes"
  ],
  "publishConfig": {
    "access": "public"
  },
  "jest": {
    "preset": "@4c/jest-preset",
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "./test/setup.ts"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "devDependencies": {
    "@4c/cli": "^2.2.1",
    "@4c/jest-preset": "^1.5.0",
    "@4c/tsconfig": "^0.3.1",
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.14",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-typescript": "^7.13.0",
    "@types/enzyme": "^3.10.5",
    "@types/jest": "^26.0.22",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.0",
    "cpy": "^8.1.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint-config-4catalyzer-jest": "^2.0.5",
    "eslint-config-4catalyzer-react": "^1.0.5",
    "eslint-config-4catalyzer-typescript": "^3.0.3",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^24.3.2",
    "eslint-plugin-react": "^7.23.1",
    "glob": "^7.1.6",
    "jest": "^26.4.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "dependencies": {
    "@restart/hooks": "^0.3.26",
    "@types/classnames": "^2.2.9",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/react-is": "^17.0.0",
    "acorn": "^8.1.0",
    "acorn-jsx": "^5.2.0",
    "classnames": "^2.2.6",
    "magic-string": "^0.25.6",
    "prism-react-renderer": "^1.2.0",
    "prop-types": "^15.7.2",
    "react-is": "^17.0.2",
    "react-simple-code-editor": "^0.11.0",
    "sourcemap-codec": "^1.4.8"
